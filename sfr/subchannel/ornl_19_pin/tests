[Tests]
  # Syntax Checks for 19-pin model
  [channel_syntax]
    type = RunApp
    input = ornl_19_pin.i
    check_input = True
    executable_pattern = 'subchannel*|pronghorn*|blue_crab*'
  []
  [viz_syntax]
    type = RunApp
    input = ornl_19_pin_viz.i
    check_input = True
    executable_pattern = 'subchannel*|pronghorn*|blue_crab*'
  []
  # Regression checks for 19-pin model
  [channel]
    type = Exodiff
    input = ornl_19_pin.i
    exodiff = 'ornl_19_pin_out.e ornl_19_pin_out_viz0.e'
    prereq = 'channel_syntax'
    executable_pattern = 'subchannel*|pronghorn*|blue_crab*'
  []
[]
